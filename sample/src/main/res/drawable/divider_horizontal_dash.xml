<?xml version="1.0" encoding="utf-8"?>

<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape>
            <solid android:color="#A9EAAC" />
        </shape>
    </item>
    <item>
        <shape android:shape="line">
            <solid android:color="@color/background" />
